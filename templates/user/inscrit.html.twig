
{% extends 'inscription.html.twig' %}

{% block title%} Inscripiton {% endblock %}

{% block body %}
<div class="row justify-content-center">
    
    <div class="container">
        <div class="row justify-content-center">
    
 {{form_start(form, {'attr': {'novalidate': 'novalidate'}} )}}


 <div class="row">
    <div class="col-sm-3">
        {{ form_label(form.nom, 'Last Name') }}
        {{ form_widget(form.nom) }}
        {{ form_errors(form.nom) }}
    </div>
    <div class="col-sm-3">
        {{ form_label(form.prenom, 'First Name') }}
        {{ form_widget(form.prenom) }}
        {{ form_errors(form.prenom) }}
    </div>
</div>


 <div class="row">
    <div class="col-sm-3 ">
     {{ form_label(form.adresse, 'Address') }}
        {{ form_widget(form.adresse) }}
         {{ form_errors(form.adresse) }}
    </div>
    <div class="col-sm-3">
        {{ form_label(form.telephone, 'Phone_Number') }}
        {{ form_widget(form.telephone) }}
         {{ form_errors(form.telephone) }}
    </div>
</div>



       <div class="row ">
    <div class="col-sm-6 ">
     {{ form_label(form.email,'Email') }}
     
     {{ form_widget(form.email) }}
                                
       
      {{ form_errors(form.email) }}
    
</div>   


 <div class="row mt-4">
    <div class="col-sm-6">
        
          
            {{ form_widget(form.mot_de_passe) }}
            {{ form_errors(form.mot_de_passe) }}
     
    </div>
</div>


    
</div> 




{{form_end(form)}}






</div>
{% endblock %}

